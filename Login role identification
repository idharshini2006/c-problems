#include<stdio.h>
int main() {
    int a;
    scanf("%d",&a);
    switch (a) {
        case 1:
            printf("Admin");
            break;
        case 2:
            printf("User");
            break;
        case 3:
            printf("Guest");
            break;
        default:
            printf("Invaid input");
            break;
    }
    return 0;
}

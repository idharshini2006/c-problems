#include<stdio.h>
int main(){
    int planType;
    scanf("%d",&planType);
    switch (planType) {
        case 1:
            printf("speed 40 Mbps");
            break;
        case 2:
            printf("speed 100 Mbps");
            break;
        case 3:
            printf("speed 300 Mbps");
            break;
        default:
            printf("Error! please enter 1 to 3");
            break;
    }
    return 0;
}

#include<stdio.h>
int main() {
    int category;
    scanf("%d",&category);
    switch (category) {
        case 1:
            printf("Exam Fee Rs.1200");
            break;
        case 2:
            printf("Exam Fee Rs.800");
            break;
        case 3:
            printf("Exam Fee Rs.500");
            break;
    }
    return 0;
}

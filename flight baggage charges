#include<stdio.h>
int main() {
    int class,weight;
    scanf("%d %d",&class,&weight);
    switch (class) {
        case 1:
            printf("Extra Baggage Charge %d",weight*300);
            break;
        case 2:
            if (weight==3) {
                printf("Free");
            }
            else {
                printf("Extra Baggage Charge %d",(weight-3)*300);
            }
            break;
    }
    return 0;
}

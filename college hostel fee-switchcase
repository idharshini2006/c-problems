#include<stdio.h>
int main() {
    int roomType;
    scanf("%d",&roomType);
    switch (roomType) {
        case 1:
            printf("Hostel Fee Rs.80000");
            break;
        case 2:
            printf("Hostel Fee Rs.60000");
            break;
        case 3:
            printf("Hostel Fee Rs.45000");
            break;
        default:
            printf("please enter 1 to 3");
            break;
    }
    return 0;
}

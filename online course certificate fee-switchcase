#include<stdio.h>
int main(){
    int courseType;
    scanf("%d",&courseType);
    switch (courseType) {
        case 1:
            printf("Certificate fee Rs.0");
            break;
        case 2:
            printf("Certificate fee Rs.500");
            break;
        default:
            printf("error please enter 1 or 2");
            break;
    }
    return 0;
}

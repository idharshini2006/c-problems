#include<stdio.h>
int main() {
    int accountType,balance,withdrawAmount;
    scanf("%d %d %d",&accountType,&balance,&withdrawAmount);
    switch (accountType) {
        case 1:
            if (balance>=withdrawAmount) {
                printf("Transaction Succesfull");
            }
        case 2:
            if (withdrawAmount>5000) {
                printf("Limit Exceeded");
            }
    }
    return 0;
}

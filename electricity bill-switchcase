#include<stdio.h>
int main() {
    int delayDays;
    scanf("%d",&delayDays);
    if (delayDays<=5) {
        printf("Fine Rs.%d",delayDays*50);
    }
    else if (delayDays<=10) {
        printf("Fine Rs.%d",delayDays*100);
    }
    else {
        printf("Fine Rs.%d",delayDays*200);
    }
    return 0;
}
